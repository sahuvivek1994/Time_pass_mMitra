<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_homeactivity"
        tools:context="tech.inscripts.ins_armman.mMitra.homeactivity.MainActivity">

<ImageButton android:layout_width="0dp"
             android:layout_height="0dp"
             android:text="button1"
             style="@style/myImageView"
             android:scaleType="centerInside"
             android:adjustViewBounds="true"
             android:id="@+id/btnRegistration"
             android:src="@drawable/woman"
             android:layout_marginTop="26dp" app:layout_constraintTop_toTopOf="@+id/guideline8"
             android:layout_marginEnd="38dp" app:layout_constraintEnd_toStartOf="@+id/guideline9"
             android:layout_marginRight="38dp" android:layout_marginBottom="26dp"
             app:layout_constraintBottom_toTopOf="@+id/guideline10" android:layout_marginStart="38dp"
             app:layout_constraintStart_toStartOf="@+id/guideline22" android:layout_marginLeft="38dp"
             app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"/>

<ImageButton android:layout_width="0dp"
             android:layout_height="0dp"
             android:id="@+id/btnCompleteForm"
             android:adjustViewBounds="true"
             android:scaleType="centerInside"
             style="@style/myImageView"
             android:src="@drawable/completed"
             android:layout_marginTop="26dp"
             app:layout_constraintTop_toBottomOf="@+id/guideline8"
             android:layout_marginBottom="26dp"
             app:layout_constraintBottom_toTopOf="@+id/guideline10"
             app:layout_constraintVertical_bias="0.0" android:layout_marginEnd="38dp"
             app:layout_constraintEnd_toStartOf="@+id/guideline25" android:layout_marginRight="38dp"
             android:layout_marginStart="38dp" app:layout_constraintStart_toStartOf="@+id/guideline9"
             android:layout_marginLeft="38dp" app:layout_constraintHorizontal_bias="0.0"/>
<ImageButton android:layout_width="0dp"
             android:layout_height="0dp"
             android:src="@drawable/incomplete"
             android:id="@+id/btnIncompleteForm"
             android:adjustViewBounds="true"
             android:scaleType="centerInside"
             style="@style/myImageView" android:layout_marginTop="26dp"
             app:layout_constraintTop_toBottomOf="@+id/guideline11"
             android:layout_marginBottom="26dp"
             app:layout_constraintBottom_toTopOf="@+id/guideline12"
             app:layout_constraintVertical_bias="0.0" android:layout_marginEnd="38dp"
             app:layout_constraintEnd_toStartOf="@+id/guideline9" android:layout_marginRight="38dp"
             android:layout_marginStart="38dp" app:layout_constraintStart_toStartOf="@+id/guideline22"
             android:layout_marginLeft="38dp" app:layout_constraintHorizontal_bias="0.0"/>
<ImageButton android:layout_width="0dp"
             android:layout_height="0dp"
             android:id="@+id/btnUserProfile"
             android:src="@drawable/user"
             android:adjustViewBounds="true"
             android:scaleType="centerInside"
             style="@style/myImageView"
             android:layout_marginLeft="38dp" android:layout_marginStart="38dp"
             app:layout_constraintStart_toStartOf="@+id/guideline9" android:layout_marginTop="26dp"
             app:layout_constraintTop_toBottomOf="@+id/guideline11"
             android:layout_marginBottom="26dp" app:layout_constraintBottom_toTopOf="@+id/textView6"
             android:layout_marginEnd="38dp" app:layout_constraintEnd_toStartOf="@+id/guideline25"
             android:layout_marginRight="38dp" app:layout_constraintHorizontal_bias="0.0"
             app:layout_constraintVertical_bias="0.0"/>
<android.support.constraint.Guideline
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/guideline8"
        app:layout_constraintGuide_percent="0.02"/>
<android.support.constraint.Guideline
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/guideline9"
        app:layout_constraintGuide_percent="0.5"/>
<android.support.constraint.Guideline
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline10" app:layout_constraintGuide_percent="0.28"/>
    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline11" app:layout_constraintGuide_percent="0.34"/>
    <TextView
            android:text="@string/btn_registration"
            android:gravity="center"
            android:textStyle="bold"
            app:fontFamily="monospace"
            android:textSize="20sp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView2"
            app:layout_constraintTop_toTopOf="@+id/guideline10"
            app:layout_constraintEnd_toStartOf="@+id/guideline9" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline11"
            app:layout_constraintVertical_bias="0.0" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline22" android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="@string/btn_complete_visit"
            android:gravity="center"
            android:textStyle="bold"
            app:fontFamily="monospace"
            android:textSize="20sp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView3" app:layout_constraintStart_toStartOf="@+id/guideline9"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline11" app:layout_constraintTop_toTopOf="@+id/guideline10"
            app:layout_constraintVertical_bias="0.0" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline25" android:layout_marginRight="8dp"/>
    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline12" app:layout_constraintGuide_percent="0.6"/>
    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline13" app:layout_constraintGuide_percent="0.66"/>
    <TextView
            android:text="@string/btn_incomplete_visit"
            android:gravity="center"
            android:textStyle="bold"
            app:fontFamily="monospace"
            android:textSize="20sp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView4"
            app:layout_constraintEnd_toStartOf="@+id/guideline9"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline12" app:layout_constraintBottom_toTopOf="@+id/guideline13"
            app:layout_constraintVertical_bias="0.0" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline22" android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="@string/btn_user_details"
            android:gravity="center"
            android:textStyle="bold"
            app:fontFamily="monospace"
            android:textSize="20sp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView6" app:layout_constraintStart_toStartOf="@+id/guideline9"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline13"
            app:layout_constraintTop_toTopOf="@+id/guideline12" app:layout_constraintVertical_bias="0.0"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline25"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"/>
<android.support.constraint.Guideline
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/guideline22"
        app:layout_constraintGuide_percent="0.03826955"/>
<android.support.constraint.Guideline
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline25" app:layout_constraintGuide_percent="0.97"/>

</android.support.constraint.ConstraintLayout>