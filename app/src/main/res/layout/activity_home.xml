<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
android:background="@color/white">
    <android.support.v7.widget.CardView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_columnWeight="1"
            app:layout_rowWeight="1"
            app:cardCornerRadius="50dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline8"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline9"
            android:layout_marginRight="8dp" android:id="@+id/cardView" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline10">
        <ImageButton android:layout_width="match_parent"
                     android:layout_height="match_parent"
                     android:text="button1"
                     android:id="@+id/btnRegistration"
                     android:src="@drawable/woman"
                     android:background="@drawable/grid_btn_bg"/>
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_columnWeight="1"
            app:layout_rowWeight="1"
            android:layout_marginRight="8dp"
            app:cardCornerRadius="50dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="@+id/guideline9"
            android:layout_marginLeft="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline10" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline8" android:id="@+id/cardView2">
        <ImageButton android:layout_width="match_parent"
                     android:layout_height="match_parent"
                     android:id="@+id/btnCompleteForm"
                     android:src="@drawable/incomplete"
                     android:background="@drawable/grid_btn1_bg"/>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_columnWeight="1"
            app:layout_rowWeight="1"
            app:cardCornerRadius="50dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline12"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline11"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline9"
            android:layout_marginRight="8dp">
        <ImageButton android:layout_width="match_parent"
                     android:layout_height="match_parent"
                     android:src="@drawable/completed"
                     android:id="@+id/btnIncompleteForm"
                     android:background="@drawable/grid_btn2_bg"/>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_columnWeight="1"
            app:layout_rowWeight="1"
            android:layout_marginRight="8dp"
            app:cardCornerRadius="50dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="@+id/guideline9"
            android:layout_marginLeft="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline12"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline11">
        <ImageButton android:layout_width="match_parent"
                     android:layout_height="match_parent"
                     android:id="@+id/btnUserProfile"
                     android:src="@drawable/user"
                     android:background="@drawable/grid_btn3_bg"/>
    </android.support.v7.widget.CardView>
    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline8"
            app:layout_constraintGuide_percent="0.1"/>
    <android.support.constraint.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline9"
            app:layout_constraintGuide_percent="0.5"/>
    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline10" app:layout_constraintGuide_percent="0.4"/>
    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline11" app:layout_constraintGuide_percent="0.46"/>
    <TextView
            android:text="REGISTRATION"
            android:gravity="center"
            android:textStyle="bold"
            app:fontFamily="serif"
            android:textSize="18sp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline10" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="4dp" android:layout_marginStart="4dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline9" android:layout_marginEnd="4dp"
            android:layout_marginRight="4dp" app:layout_constraintBottom_toTopOf="@+id/guideline11"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="INCOMPLETE FROM"
            android:gravity="center"
            android:textStyle="bold"
            app:fontFamily="serif"
            android:textSize="18sp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView3" app:layout_constraintStart_toStartOf="@+id/guideline9"
            android:layout_marginLeft="4dp" android:layout_marginStart="4dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="4dp" android:layout_marginRight="4dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline11" app:layout_constraintTop_toTopOf="@+id/guideline10"
            android:layout_marginTop="8dp" app:layout_constraintVertical_bias="0.0"/>
    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline12" app:layout_constraintGuide_percent="0.76"/>
    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline13" app:layout_constraintGuide_percent="0.82"/>
    <TextView
            android:text="COMPLETE FORM"
            android:gravity="center"
            android:textStyle="bold"
            app:fontFamily="serif"
            android:textSize="18sp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView4" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp" app:layout_constraintEnd_toStartOf="@+id/guideline9"
            android:layout_marginEnd="4dp" android:layout_marginRight="4dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline12" app:layout_constraintBottom_toTopOf="@+id/guideline13"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="USER DETAILS"
            android:gravity="center"
            android:textStyle="bold"
            app:fontFamily="serif"
            android:textSize="18sp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView6" app:layout_constraintStart_toStartOf="@+id/guideline9"
            android:layout_marginLeft="4dp" android:layout_marginStart="4dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="4dp" android:layout_marginRight="4dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline13" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline12" app:layout_constraintVertical_bias="0.0"/>

</android.support.constraint.ConstraintLayout>